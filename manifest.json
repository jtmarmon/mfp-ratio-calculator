{
   "version": "1",
   "manifest_version": 2,
   "name": "My Fitness Pal Ratio Calculator",
   "short_name": "MFP Ratios",
   "description": "Calculates fat/protein/carb ratio on my fitness pal",
   "icons": {
       "48": "icon/mfp_48.png",
       "128": "icon/mfp_128.png"
   },
   "content_scripts": [{
       "js": [ "ratios.js" ],
       "css": [ "ratios.css" ],
       "matches": [ "http://www.myfitnesspal.com/food/diary" ]
   }]
}
